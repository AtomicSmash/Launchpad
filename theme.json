{
	"$schema": "https://raw.githubusercontent.com/WordPress/gutenberg/wp/6.3/schemas/json/theme.json",
	"version": 2,
	"settings": {
		"color": {
			"background": true,
			"custom": false,
			"customDuotone": false,
			"customGradient": false,
			"defaultDuotone": false,
			"defaultGradients": false,
			"defaultPalette": false,
			"duotone": [],
			"gradients": [],
			"text": true,
			"palette": [
				{
					"slug": "bright-grey",
					"name": "Bright Grey",
					"color": "#ECECEC"
				},
				{
					"slug": "dark-grey",
					"name": "Dark Grey",
					"color": "#BDBDBD"
				},
				{
					"slug": "charcoal",
					"name": "Charcoal",
					"color": "#2B2B2B"
				},
				{
					"slug": "white",
					"name": "White",
					"color": "#FEFFFE"
				},
				{
					"slug": "black",
					"name": "Black",
					"color": "#000"
				},
				{
					"slug": "required",
					"name": "Required",
					"color": "#D23636"
				}
			]
		},
		"typography": {
			"customFontSize": false,
			"dropCap": false,
			"fontSizes": [
				{
					"name": "Small",
					"slug": "small",
					"size": "0.875rem",
					"fluid": false
				},
				{
					"name": "Medium",
					"slug": "medium",
					"size": "1rem",
					"fluid": false
				},
				{
					"name": "Large",
					"slug": "large",
					"size": "1.125rem",
					"fluid": false
				},
				{
					"name": "Extra Large",
					"slug": "x-large",
					"size": "clamp(1.25rem, calc(1.25rem + ((1vw - 0.25rem) * 0.2703)), 1.375rem)"
				},
				{
					"name": "Heading 1",
					"slug": "heading-1",
					"size": "clamp(2.125rem, 1.491rem + 3.171vw, 3.75rem)"
				},
				{
					"name": "Heading 2",
					"slug": "heading-2",
					"size": "clamp(1.875rem, calc(1.875rem + ((1vw - 0.25rem) * 1.6216)), 2.625rem)"
				},
				{
					"name": "Heading 3",
					"slug": "heading-3",
					"size": "clamp(1.5rem, calc(1.5rem + ((1vw - 0.25rem) * 0.8108)), 1.875rem)"
				}
			],
			"fontFamilies": [
				{
					"fontFamily": "'Poppins', system-ui",
					"slug": "poppins",
					"name": "Poppins",
					"fontFace": [
						{
							"fontFamily": "Poppins",
							"fontWeight": "400",
							"fontStyle": "normal",
							"fontDisplay": "swap",
							"src": ["file:./static/v1/fonts/Poppins-Regular.woff2"]
						},
						{
							"fontFamily": "Poppins",
							"fontWeight": "700",
							"fontStyle": "normal",
							"fontDisplay": "swap",
							"src": ["file:./static/v1/fonts/Poppins-Bold.woff2"]
						},
						{
							"fontFamily": "Poppins",
							"fontWeight": "400",
							"fontStyle": "italic",
							"fontDisplay": "swap",
							"src": ["file:./static/v1/fonts/Poppins-Italic.woff2"]
						},
						{
							"fontFamily": "Poppins",
							"fontWeight": "700",
							"fontStyle": "italic",
							"fontDisplay": "swap",
							"src": ["file:./static/v1/fonts/Poppins-BoldItalic.woff2"]
						}
					]
				}
			],
			"fontStyle": true,
			"fontWeight": true,
			"letterSpacing": false,
			"textColumns": false,
			"textDecoration": true,
			"textTransform": true
		},
		"spacing": {
			"customSpacingSize": false,
			"spacingScale": {},
			"spacingSizes": [
				{
					"name": "Small",
					"size": "0.5rem",
					"slug": "small"
				},
				{
					"name": "Normal",
					"size": "1rem",
					"slug": "normal"
				},
				{
					"name": "Large",
					"size": "1.25rem",
					"slug": "large"
				},
				{
					"name": "XL",
					"size": "2.5rem",
					"slug": "x-large"
				},
				{
					"name": "XXL",
					"size": "5rem",
					"slug": "xx-large"
				}
			],
			"units": ["px", "em", "rem", "vh", "vw", "%"]
		},
		"custom": {
			"site-container-x-padding": "clamp(0.75rem, calc(0.36rem + 1.95vw), 1.75rem)",
			"site-header-y-padding": "clamp(1.25rem, 1.0549rem + 0.9756vw, 1.75rem)",
			"site-footer-y-padding": "clamp(1.25rem, 1.0549rem + 0.9756vw, 1.75rem)",
			"nav-item-gap": "clamp(0.75rem, calc(0.36rem + 1.95vw), 1.75rem)",
			"contact-form-padding": "clamp(1.25rem, 0.762rem + 2.439vw, 2.5rem)"
		},
		"layout": {
			"contentSize": "calc(75rem - (var(--wp--custom--site-container-x-padding) * 2))"
		},
		"appearanceTools": true,
		"useRootPaddingAwareAlignments": true,
		"shadow": {
			"defaultPresets": false,
			"presets": []
		}
	},
	"styles": {
		"spacing": {
			"blockGap": "var(--wp--preset--spacing--x-large)",
			"padding": {
				"top": "0",
				"right": "var(--wp--custom--site-container-x-padding)",
				"bottom": "0",
				"left": "var(--wp--custom--site-container-x-padding)"
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--poppins)",
			"fontSize": "1rem",
			"lineHeight": "1.5"
		},
		"blocks": {
			"core/paragraph": {
				"spacing": {
					"margin": {
						"top": "0",
						"right": "0",
						"bottom": "0.5em",
						"left": "0"
					}
				}
			}
		},
		"color": {
			"text": "var(--wp--preset--color--charcoal)"
		},
		"elements": {
			"link": {
				"color": {
					"text": "var(--wp--preset--color--charcoal)"
				},
				"typography": {
					"textDecoration": "underline"
				},
				":hover": {
					"color": {
						"text": "var(--wp--preset--color--charcoal)"
					},
					"typography": {
						"textDecoration": "none"
					}
				}
			}
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header",
			"title": "Header"
		},
		{
			"area": "footer",
			"name": "footer",
			"title": "Footer"
		}
	]
}
